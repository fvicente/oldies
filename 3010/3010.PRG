SET SCOREBOARD OFF
SET WRAP ON
VAR=0

DO WHILE (.T.)

SET COLOR TO "N/N"
CLEAR
BOX(20,5,58,14)
SET COLOR TO "W+/BG, W+/N"

SET COLOR TO ("W+/BG")
@ 6, 21 SAY "           MENU  PRINCIPAL           "

SET COLOR TO ("N/W")
@  8, 21 PROMPT " 1.- CALCULAR DIODOS                 "
@  9, 21 PROMPT " 2.- CALCULAR FRECUENCIA             "
@ 10, 21 PROMPT " 3.- CARACTERISTICAS DEL SM-3010     "
@ 12, 21 PROMPT " 4.- SALIR                           "

MENU TO VAR
DO CASE
	CASE VAR==1
		SET COLOR TO "N/N"
		DO DIO3010
	CASE VAR==2
		SET COLOR TO "N/N"
		DO FRE3010
	CASE VAR==3
		SET COLOR TO "N/N"
		DO CAR3010
	CASE VAR==4
		SET COLOR TO ("")
		CLEAR
		QUIT
	CASE VAR==0
		SET COLOR TO ("")
		CLEAR
		QUIT
ENDCASE

ENDDO
RETURN